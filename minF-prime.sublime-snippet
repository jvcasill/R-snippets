#-----Calculate " min F' "-----#

library(devtools)
source_url("https://gist.githubusercontent.com/mmarascio/f4399fb4206afb8bdb39/raw/")

minF.fnc(
	f.1=${1: "F"-ratio for given factor from by-subjects ANOVA},
	df.1=c(${2: degrees of freedom_numerator thereof},${3: degrees of freedom_denominator thereof}),
	f.2=${4: "F"-ratio for same factor from by-items ANOVA},
	df.2=c(${5: degrees of freedom_numerator thereof},${6: degrees of freedom_denominator thereof})
)
${0}

]]></content>
